<div id="form">
  <form id="productForm">
    <div class="form-group">
      <label for="nombre">Nombre</label>
      <input id="title" class="form-control" type="text" name="title" required>
    </div>

    <div class="form-group">
      <label for="nombre">Descripcion</label>
      <input id="description" class="form-control" type="text" name="description" required>
    </div>

    <div class="form-group">
      <label for="nombre">Codigo</label>
      <input id="code" class="form-control" type="text" name="code" required>
    </div>
    <div class="form-group">
      <label for="precio">Precio</label>
      <input id="price" class="form-control" type="number" step="0.5" name="price" required>
    </div>

    <div class="form-group">
      <label for="nombre">Stock</label>
      <input id="stock" class="form-control" type="text" name="stock" required>
    </div>
    <div class="form-group">
      <label for="foto">Categoria</label>
      <input id="category" class="form-control" type="text" name="category">
    </div>
    <button class="btn btn-success mt-3">Enviar</button>
  </form>

</div>

<div id="productList">
</div>

<script src="socket.io/socket.io.js"></script>
<script src="/src/public/js/index.js"></script>